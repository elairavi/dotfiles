
## Source files in zsh config ##
source ~/.config/zsh/configs/aliases.zsh
source ~/.config/zsh/configs/completion.zsh
source ~/.config/zsh/configs/functions.zsh
source ~/.config/zsh/configs/fzf-bindings.zsh
source ~/.config/zsh/configs/general.zsh
source ~/.config/zsh/configs/prompt.zsh
